{
  "name": "tyval",
  "version": "4.0.0",
  "description": "Fast and extensible validator for JavaScript",
  "main": "tyval.js",
  "scripts": {
    "bench": "npm run pretest && node bench/bench.js",
    "pretest": "standard",
    "test": "tap test/*.js",
    "test:all": "./test/test-runner.sh",
    "browser": "node build-browser.js"
  },
  "keywords": [
    "fast",
    "validator",
    "validation",
    "extensible",
    "composable",
    "type",
    "variable",
    "check",
    "value"
  ],
  "author": "Tomas Della Vedova - @delvedor (http://delved.org)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delvedor/Tyval.git"
  },
  "bugs": {
    "url": "https://github.com/delvedor/Tyval/issues"
  },
  "homepage": "https://github.com/delvedor/Tyval#readme",
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "benchmark": "^2.1.1",
    "browserify": "^13.1.0",
    "chalk": "^1.1.3",
    "joi": "^9.1.1",
    "replacestream": "^4.0.2",
    "self-stream": "^1.1.1",
    "standard": "^8.3.0",
    "tap": "^7.1.2",
    "uglify-js": "^2.7.3",
    "validate.js": "^0.10.0"
  },
  "dependencies": {}
